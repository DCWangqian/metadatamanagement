                                    <section layout="row" layout-xs="column" layout-align-xs="start start">
                                        <md-button class="md-primary md-hue-1" href="fakefiles/Abs05_W2_Fragebogen.pdf" download>Fragebogen</md-button>
                                        <md-button class="md-primary md-hue-1" href="fakefiles/Abs05_W2_Variablenfragebogen.pdf" download>Variablenfragebogen</md-button>
                                        <md-button class="md-primary md-hue-1" href="fakefiles/Abs05_W2_Filtergrafik.pdf" download>Filterführung</md-button>
                                        <md-button class="md-primary md-hue-1" href="fakefiles/Abs05_W2_Pretest_Fragebogen.pdf" download>Pretestfragebogen</md-button>
                                        <md-button class="md-primary md-hue-1" href="fakefiles/Abs05_W2_Pretest_Fragenkatalog.pdf" download>Fragenkatalog Pretest</md-button>
                                    </section>
                                </div>
                            </md-tab-body>
                        </md-tab>
                    </md-tabs>
                </md-card-content>
            </md-card>
        </md-card-content>
    </md-card>
</div>
