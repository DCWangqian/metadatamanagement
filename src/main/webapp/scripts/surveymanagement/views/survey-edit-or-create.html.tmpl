                    <md-input-container flex="50" style="-ms-flex:auto" ng-class="{'fdz-add-right-margin': $mdMedia('gt-sm')}" md-is-error="!surveyForm.sampleDe.$valid && (surveyForm.sampleDe.$dirty || surveyForm.sampleDe.$touched)">
                        <label class="fdz-input-required">{{'survey-management.detail.label.sample' | translate}} ({{'global.in-german' | translate}})</label>
                        <input md-maxlength="2048" fdz-required="!ctrl.survey.sample.en" ng-model="ctrl.survey.sample.de" name="sampleDe" lang="de" md-no-asterisk>
                        <div ng-if="!surveyForm.sampleDe.$dirty && !surveyForm.sampleDe.$touched" class="fdz-input-hint">{{'survey-management.edit.hints.sample.de' | translate}}</div>
                        <div ng-messages="surveyForm.sampleDe.$error" multiple>
                            <div ng-message="fdz-required">{{'survey-management.error.survey.sample.i18n-string-not-empty' | translate}}</div>
                            <div ng-message="md-maxlength">{{'survey-management.error.survey.sample.i18n-string-size' | translate}}</div>
                        </div>
                    </md-input-container>
                    <md-input-container flex="50" style="-ms-flex:auto" md-is-error="!surveyForm.sampleEn.$valid && (surveyForm.sampleEn.$dirty || surveyForm.sampleEn.$touched)">
                        <label class="fdz-input-required">{{'survey-management.detail.label.sample' | translate}} ({{'global.in-english' | translate}})</label>
                        <input md-maxlength="2048" fdz-required="!ctrl.survey.sample.de" ng-model="ctrl.survey.sample.en" name="sampleEn" lang="en" md-no-asterisk>
                        <div ng-if="!surveyForm.sampleEn.$dirty && !surveyForm.sampleEn.$touched" class="fdz-input-hint">{{'survey-management.edit.hints.sample.en' | translate}}</div>
                        <div ng-messages="surveyForm.sampleEn.$error" multiple>
                            <div ng-message="fdz-required">{{'survey-management.error.survey.sample.i18n-string-not-empty' | translate}}</div>
                            <div ng-message="md-maxlength">{{'survey-management.error.survey.sample.i18n-string-size' | translate}}</div>
                        </div>
                    </md-input-container>
                </div>
                <div layout-gt-sm="row" layout="column">
                    <md-input-container flex="50" style="-ms-flex:auto" md-is-error="!surveyForm.grossSampleSize.$valid && (surveyForm.grossSampleSize.$dirty || surveyForm.grossSampleSize.$touched)">
                        <label>{{'survey-management.detail.label.grossSampleSize' | translate}}</label>
                        <input ng-model="ctrl.survey.grossSampleSize" name="grossSampleSize" type="number" step="1" min="0">
                        <div ng-if="!surveyForm.grossSampleSize.$dirty && !surveyForm.grossSampleSize.$touched" class="fdz-input-hint">{{'survey-management.edit.hints.grossSampleSize' | translate}}</div>
                        <div ng-messages="surveyForm.grossSampleSize.$error" multiple>
                            <div ng-message="step">{{'survey-management.error.survey.gross-sample-size.invalid-number' | translate}}</div>
                            <div ng-message="number">{{'survey-management.error.survey.gross-sample-size.invalid-number' | translate}}</div>
                            <div ng-message="min">{{'survey-management.error.survey.gross-sample-size.min' | translate}}</div>
                        </div>
                    </md-input-container>
                    <div flex="50"></div>
                </div>
                <div layout-gt-sm="row" layout="column">
                    <md-input-container flex="50" style="-ms-flex:auto" md-is-error="!surveyForm.sampleSize.$valid && (surveyForm.sampleSize.$dirty || surveyForm.sampleSize.$touched)">
                        <label>{{'survey-management.detail.label.sampleSize' | translate}}</label>
                        <input required ng-model="ctrl.survey.sampleSize" name="sampleSize" type="number" step="1" min="0">
                        <div ng-if="!surveyForm.sampleSize.$dirty && !surveyForm.sampleSize.$touched" class="fdz-input-hint">{{'survey-management.edit.hints.sampleSize' | translate}}</div>
                        <div ng-messages="surveyForm.sampleSize.$error" multiple>
                            <div ng-message="required">{{'survey-management.error.survey.sample-size.not-null' | translate}}</div>
                            <div ng-message="step">{{'survey-management.error.survey.sample-size.invalid-number' | translate}}</div>
                            <div ng-message="number">{{'survey-management.error.survey.sample-size.invalid-number' | translate}}</div>
                            <div ng-message="min">{{'survey-management.error.survey.sample-size.min' | translate}}</div>
                        </div>
                    </md-input-container>
                    <div flex="50"></div>
                </div>
                <div layout-gt-sm="row" layout="column">
                    <md-input-container flex="50" style="-ms-flex:auto" md-is-error="!surveyForm.responseRate.$valid && (surveyForm.responseRate.$dirty || surveyForm.responseRate.$touched)">
