                        <input ng-model="ctrl.survey.responseRate" name="responseRate" type="number" step="0.01" min="0" max="100">
                        <div ng-if="!surveyForm.responseRate.$dirty && !surveyForm.responseRate.$touched" class="fdz-input-hint">{{'survey-management.edit.hints.responseRate' | translate}}</div>
                        <div ng-messages="surveyForm.responseRate.$error" multiple>
                            <div ng-message="step">{{'survey-management.error.survey.response-rate.invalid-number' | translate}}</div>
                            <div ng-message="number">{{'survey-management.error.survey.response-rate.invalid-number' | translate}}</div>
                            <div ng-message="min">{{'survey-management.error.survey.response-rate.min' | translate}}</div>
                            <div ng-message="max">{{'survey-management.error.survey.response-rate.max' | translate}}</div>
                        </div>
                    </md-input-container>
                    <div flex="50"></div>
                </div>
                <div layout-gt-sm="row" layout="column">
                    <md-input-container flex="50" style="-ms-flex:auto" ng-class="{'fdz-add-right-margin': $mdMedia('gt-sm')}" md-is-error="!surveyForm.annotationsDe.$valid && (surveyForm.annotationsDe.$dirty || surveyForm.annotationsDe.$touched)">
                        <label>{{'survey-management.detail.label.annotations' | translate}} ({{'global.in-german' | translate}})</label>
                        <textarea rows="4" md-maxlength="2048" ng-model="ctrl.survey.annotations.de" name="annotationsDe" lang="de" md-no-asterisk></textarea>
                        <div ng-if="!surveyForm.annotationsDe.$dirty && !surveyForm.annotationsDe.$touched" class="fdz-input-hint">{{'survey-management.edit.hints.annotations.de' | translate}}</div>
                        <div ng-messages="surveyForm.annotationsDe.$error" multiple>
                            <div ng-message="fdz-required">{{'survey-management.error.survey.annotations.i18n-string-not-empty' | translate}}</div>
                            <div ng-message="md-maxlength">{{'survey-management.error.survey.annotations.i18n-string-size' | translate}}</div>
