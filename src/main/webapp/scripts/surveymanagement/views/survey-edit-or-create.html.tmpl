                </div>
                <div layout-gt-sm="row" layout="column" style="-ms-flex:auto">
                    <md-input-container flex="50" style="-ms-flex:auto" md-is-error="!surveyForm.dataType.$valid && (surveyForm.dataType.$dirty || surveyForm.dataType.$touched)">
                        <label>{{'survey-management.detail.label.data-type' | translate}}</label>
                        <md-select required ng-model="ctrl.survey.dataType" ng-model-options="{trackBy: '$value.de'}" name="dataType">
                            <md-option ng-value="dataType" ng-repeat="dataType in ctrl.dataTypes">{{dataType[currentLanguage]}}</md-option>
                        </md-select>
                        <div ng-if="!surveyForm.dataType.$dirty && !surveyForm.dataType.$touched" class="fdz-input-hint">{{'survey-management.edit.hints.data-type' | translate}}</div>
                        <div ng-messages="surveyForm.dataType.$error" multiple>
                            <div ng-message="required">{{'survey-management.error.survey.data-type.not-null' | translate}}</div>
                        </div>
                    </md-input-container>
                    <div flex="50"></div>
                </div>
                <div layout-gt-sm="row" layout="column">
                    <md-input-container flex="50" style="-ms-flex:auto" ng-class="{'fdz-add-right-margin': $mdMedia('gt-sm')}" md-is-error="!surveyForm.populationDescriptionDe.$valid && (surveyForm.populationDescriptionDe.$dirty || surveyForm.populationDescriptionDe.$touched)">
