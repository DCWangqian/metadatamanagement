                        <textarea rows="4" md-maxlength="2048" fdz-required ng-model="ctrl.survey.population.description.de" name="populationDescriptionDe" lang="de" md-no-asterisk></textarea>
                        <div ng-if="!surveyForm.populationDescriptionDe.$dirty && !surveyForm.populationDescriptionDe.$touched" class="fdz-input-hint">{{'survey-management.edit.hints.population.description.de' | translate}}</div>
