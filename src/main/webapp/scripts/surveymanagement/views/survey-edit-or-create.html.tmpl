                        <label class="fdz-input-required">{{'survey-management.edit.label.population.description' | translate}} ({{'global.in-english' | translate}})</label>
                        <textarea rows="4" md-maxlength="2048" fdz-required ng-model="ctrl.survey.population.description.en" name="populationDescriptionEn" lang="en" md-no-asterisk></textarea>
                        <div ng-if="!surveyForm.populationDescriptionEn.$dirty && !surveyForm.populationDescriptionEn.$touched" class="fdz-input-hint">{{'survey-management.edit.hints.population.description.en' | translate}}</div>
                        <div ng-messages="surveyForm.populationDescriptionEn.$error" multiple>
                            <div ng-message="fdz-required">{{'survey-management.error.population.description.i18n-string-not-empty' | translate}}</div>
                            <div ng-message="md-maxlength">{{'survey-management.error.population.description.i18n-string-size' | translate}}</div>
                        </div>
                    </md-input-container>
                </div>
