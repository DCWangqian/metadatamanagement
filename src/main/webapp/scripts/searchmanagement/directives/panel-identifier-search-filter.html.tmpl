<div ng-form="panelIdentifierFilterForm" layout="row" layout-align="center center">
  <md-autocomplete class="fdz-search-filter-component" md-input-name="panelIdentfierFilter" md-floating-label="Panel-Kennung" flex md-min-length="0" md-require-match="true" md-select-on-match="true" md-selected-item="currentPanelIdentifier" md-selected-item-change="onSelectionChanged(currentPanelIdentifier)" md-search-text="searchText" md-items="item in searchPanelIdentifiers(searchText)" md-item-text="item" md-clear-button="true" md-no-cache="true">
    <md-item-template>
      <span class="md-title" style="font-size: 14px;">{{item}}</span>
    </md-item-template>
    <md-not-found>
      Not found
    </md-not-found>
    <div ng-messages="panelIdentifierFilterForm.panelIdentfierFilter.$error">
        <div ng-message="md-require-match">Keine Panel-Kennung ausgew√§hlt</div>
    </div>
  </md-autocomplete>
</div>
