<md-card md-ink-ripple="true" class="fdz-search-result md-ink-ripple" ui-sref="variableDetail({id:searchResult.id})">
    <md-card-header>
        <md-card-avatar>
            <md-icon md-svg-src="/assets/images/icons/variable.svg" class="fdz-search-result-icon"></md-icon>
        </md-card-avatar>
        <md-card-header-text>
            <span class="md-title fdz-truncate-string">{{searchResult.label}}</span>
            <span class="md-subhead fdz-truncate-string">{{searchResult.id}}</span>
        </md-card-header-text>
    </md-card-header>
    <md-card-content style="padding-top: 0px;">
        <div>Datentyp: {{searchResult.dataType}}</div>
        <div>Skalenniveau: {{searchResult.scaleLevel}}</div>
    </md-card-content>
</md-card>
