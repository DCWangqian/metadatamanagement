<md-card ui-sref="questionDetail({
  id:searchResult.dataAcquisitionProjectId + '-ins' +
  searchResult.instrumentNumber + '-' + searchResult.number,
  instrumentNumber:searchResult.instrumentNumber})" class="fdz-search-result">
    <md-card-header class="fdz-card-header-border">
        <md-card-avatar>
            <md-icon md-svg-src="/assets/images/icons/question.svg" class="fdz-search-result-icon"></md-icon>
        </md-card-avatar>
        <md-card-header-text class="fdz-truncate-string">
            <div class="md-title fdz-truncate-string">{{'question-management.detail.question' | translate}} {{searchResult.number}}: {{searchResult.instrumentDescription}} ({{searchResult.id}})</div>
            <div class="md-subhead fdz-truncate-string">{{searchResult.type}}</div>
        </md-card-header-text>
    </md-card-header>
    <md-card-content>
        <div>{{searchResult.questionText}}</div>
    </md-card-content>
</md-card>
