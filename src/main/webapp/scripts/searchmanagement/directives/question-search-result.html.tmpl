<md-card md-ink-ripple="true">
  <a ui-sref="questionDetail({id:searchResult.id})" class="fdz-search-result">
    <md-card-header class="fdz-card-header-border">
        <md-card-avatar>
            <md-icon md-svg-src="/assets/images/icons/question.svg" class="fdz-search-result-icon"></md-icon>
        </md-card-avatar>
        <md-card-header-text class="fdz-truncate-string">
            <span class="md-title fdz-truncate-string">Fragebogen "TODO" ({{searchResult.dataAcquisitionProjectId}})</span>
            <span class="md-subhead fdz-truncate-string">{{searchResult.compositeQuestionName}} ({{searchResult.type}})</span>
        </md-card-header-text>
    </md-card-header>
  </a>
    <md-card-content>
      <div>{{searchResult.questionText}}</div>
    </md-card-content>
</md-card>
