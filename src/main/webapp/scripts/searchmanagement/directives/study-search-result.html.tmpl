<md-card>
  <md-card-header md-ink-ripple ui-sref="studyDetail({id: searchResult.id})" class="fdz-search-result fdz-card-header-border">
      <md-card-avatar>
          <md-icon md-svg-src="/assets/images/icons/study.svg" class="fdz-search-result-icon"></md-icon>
      </md-card-avatar>
      <md-card-header-text class="fdz-truncate-string">
          <div class="md-title fdz-truncate-string">{{searchResult.title}}</div>
          <div class="md-subhead fdz-truncate-string">{{searchResult.id}}</div>
      </md-card-header-text>
  </md-card-header>
    <md-card-content>
      <div layout="row">
        <div class="md-subhead fdz-truncate-string">{{searchResult.description}}</div>
      </div>
    </md-card-content>
</md-card>
