<md-dialog aria-label="protocol for upload">
    <md-toolbar>
        <div class="md-toolbar-tools">
            <h2>
                <span>{{'search.dialog.upload.upload-protocol' | translate}}</span>
            </h2>
            <span flex></span>
            <span class="label label-as-badge label-success">{{'search.dialog.upload.success' | translate}}:
                {{job.successes || 0}}</span>
            <span class="label label-as-badge label-danger">{{'search.dialog.upload.error' | translate}}:
                {{job.errors || 0}}</span>
            <md-button class="md-icon-button" ng-click="closeDialog()">
                <md-icon md-font-set="material-icons">clear</md-icon>
            </md-button>
        </div>
    </md-toolbar>
    <md-dialog-content>
        <div layout="row">
            <md-card flex>
                <md-card-content>
                    <pre><span ng-repeat="logMessage in job.logMessages track by $index" ng-class="{'fdz-log-error-text': logMessage.type==='error'}">{{logMessage.message}}</span></pre>
                </md-card-content>
            </md-card>
        </div>
    </md-dialog-content>
</md-dialog>
