<md-card>
	<md-card-header>
		<md-card-header-text>
			<span class="md-headline">
				{{'data-acquisition-project-management.project-cockpit.requirements.'+group | translate}}
				<span ng-if="count !== null">({{count}})</span>
				<span><md-icon>sentiment_satisfied</md-icon></span>
			</span>
		</md-card-header-text>
	</md-card-header>
	<md-card-content>
		<md-switch ng-model="project.configuration.requirements[group+'Required']" ng-disabled="true">Expected</md-switch>
		<md-switch ng-model="project.configuration[group+'State'].isDataProviderReady">Data Provider Ready</md-switch>
		<md-switch ng-model="project.configuration[group+'State'].isPublisherReady">Publisher Ready</md-switch>
	</md-card-content>
	<md-card-actions layout="row" layout-align="end center">
		<md-button>New</md-button>
	</md-card-actions>
</md-card>
